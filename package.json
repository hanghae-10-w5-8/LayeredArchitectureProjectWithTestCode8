        "express": "^4.18.2",
        "jsonwebtoken": "^8.5.1",
        "mysql2": "^2.3.3",
        "passport": "^0.6.0",
        "sequelize": "^6.27.0"
    },
    "devDependencies": {
        "eslint": "^8.29.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-prettier": "^4.2.1",
        "jest": "^29.3.1",
        "nodemon": "^2.0.20",
        "sequelize-cli": "^6.5.2"
