{
    "name": "team-project",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "scripts": {
        "test": "npx jest --forceExit",
        "test:silent": "npx jest --silent --forceExit",
        "test:coverage": "npx jest --coverage --forceExit",
        "test:unit": "npx jest __test__/unit --forceExit --passWithNoTests",
        "test:integration": "npx jest __test__/integration --forceExit --passWithNoTests",
        "lint": "eslint src",
        "dev": "nodemon src/app.js",
        "db:create": "npx sequelize db:create --config ./src/config/config.js",
        "db:migrate": "npx sequelize db:migrate --config ./src/config/config.js --migrations-path ./src/migrations --models-path ./src/models",
        "db:drop": "npx sequelize db:drop --config ./src/config/config.js"
    },
    "author": "",
